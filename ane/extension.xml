<extension xmlns="http://ns.adobe.com/air/extension/3.1">
  <id>com.kcly.ane.bitmapdataqrcodescanner</id>
  <versionNumber>1</versionNumber>
  <platforms>
    <platform name="iPhone-ARM">
            <applicationDeployment>
                <nativeLibrary>libBitmapDataQRCodeScanner.a</nativeLibrary>
                <initializer>ScannerExtInitializer</initializer>
                <finalizer>ScannerExtFinalizer</finalizer>
            </applicationDeployment>
        </platform>
    <platform name="Android-ARM">
      <applicationDeployment>
        <nativeLibrary>libBitmapDataQRCodeScanner.jar</nativeLibrary>
        <initializer>com.kcly.ane.bitmapdataqrcodescanner.ScannerExtension</initializer>
        <finalizer>com.kcly.ane.bitmapdataqrcodescanner.ScannerExtension</finalizer>
      </applicationDeployment>
    </platform>
  </platforms>
</extension>